{
    "name": "zapmeteam/api",
    "description": "ZapMe API it's the official client API from ZapMe",
    "keywords": [
        "zapme",
        "zapmeapi"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ZapMeTeam",
            "email": "dev@zapme.com.br"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "vlucas/phpdotenv": "^5.3",
        "fakerphp/faker": "^1.16",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "ZapMeTeam\\Api\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
